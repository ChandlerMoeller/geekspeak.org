<%
if params[:action] == 'year_archive'
	 title "#{params[:year]}"
elsif params[:action] == 'month_archive'
	 title "#{Date::MONTHNAMES[params[:month].to_i]} of #{params[:year]}"
end 
%>
<% title  " Episodes" %>
<% for episode in @episodes %>
	<%= render :partial => "episodes/episode", :object => episode %>
<% end %>

